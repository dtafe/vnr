                                            @(Html.VnrComboBoxFor(mode => mode.CurenncyID5, CurenncyOAllowanceName).Text(Model == null ? string.Empty : Model.CurenncyOAllowanceName))
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </td>
            </tr>
