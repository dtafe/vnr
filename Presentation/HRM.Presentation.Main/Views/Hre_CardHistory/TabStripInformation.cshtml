@using HRM.Presentation.UI.Controls.Kendo.TabStrip;
@using HRM.Infrastructure.Utilities;


@{
    var tab = new TabStripInfomation
    {
        Name = "tabStripInOut",
    };

    var t1 = new ItemsInformation
    {
        Content = PartialExtensions.Partial(@Html, "CreateOrUpdate", Model).ToHtmlString(),
        Selected = true
    };
    var t2 = new ItemsInformation
    {
        Content = PartialExtensions.Partial(@Html, "Detail", Model).ToHtmlString(),
        Selected = false
    };
 

    tab.TabItems.Add(ConstantDisplay.HRM_Hr_CardHistory_Info.TranslateString(), t1);
    //tab.TabItems.Add(ConstantDisplay.HRM_Hr_CardHistory_InfoDetail.TranslateString(), t2);

}
@(Html.TabStripControl(tab))
