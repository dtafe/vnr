@using HRM.Infrastructure.Utilities;
@using HRM.Presentation.UI.Controls.Kendo

@model HRM.Presentation.Category.Models.CatImportModel
@{
    ViewBag.Title = ConstantDisplay.HRM_Category_Export_AddNew.TranslateString();
    Layout = null;
    var lineId = Model == null ? Guid.Empty : Model.ID;
    var _hrm_Sys_Service = ConstantPathWeb.Hrm_Sys_Service;
    var _hrm_Main_Web = ConstantPathWeb.Hrm_Main_Web;
    var _hrm_Hr_Service = ConstantPathWeb.Hrm_Hre_Service;
}
<script>

    function doSave() {
        var result = false;
        onCreate($('#form-create'), '@_hrm_Hr_Service', 'api/CatImport/', "popupNotification"); result = true;
        return result;
    }

    function doSaveNew() {
        debugger;
        if (doSave()) {
            KendoRouteMapping('#Hrm_Main_Web/Cat_Import/Create/', 'Hrm_Main_Web', '@_hrm_Main_Web');
        }
    }

</script>
<div>
    @ViewBag.MsgInsert
</div>

<form action="#" method="POST" id="form-create">
    <div class="panel-wrap">
        <div id="main-content">
            <div class="widget">
                <div class="section-header">
                    <div class="title">
                        <img src="@ConstantPath.ExportIconPath" alt="" width="20" height="20" />
                        @ConstantDisplay.HRM_Category_Cat_Import_AddNew.TranslateString()
                        @Html.ActionLink("(" + ConstantDisplay.HRM_Category_Back_To_Cat_Import_List.TranslateString() + ")", "Index")
                    </div>
                </div>

                <div class="commandToolBarWidge">
                    <div class="options">
                        @Html.VnrButton(new ButtonBuilderInfo() { EventClick = "doSave", Name = "save_Import", Content = ConstantAction.HRM_Common_Save.TranslateString(), ImageUrl = ConstantPath.IconSavePath })
                        @Html.VnrButton(new ButtonBuilderInfo() { EventClick = "doSaveNew", Name = "save-continue_Import", Content = ConstantAction.HRM_Common_SaveContinue.TranslateString(), ImageUrl = ConstantPath.IconSavePath })
                    </div>
                </div>
                @Html.Partial("CreateOrUpdate", Model)
            </div>
        </div>
    </div>
</form>





