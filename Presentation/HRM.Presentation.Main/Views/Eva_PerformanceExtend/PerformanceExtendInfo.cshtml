<script>
    SetFieldRequired();
    var PerformanceID = "";
    $(document).ready(function () {
        if ('@PerformanceID' != '@Guid.Empty') {
            PerformanceID = '@PerformanceID';
        }
        else if ($("#PerformanceID").val() == "" || $("#PerformanceID").val() == undefined) {
            PerformanceID = $("#PerformanceID").val();
        }

    })
    function UpdatePerformanceExtendIDOfPerformance()
    {
        

        $.ajax({
            traditional: true,
            url: '@_Hrm_Hre_Service' + 'Eva_GetData/UpdatePerformanceExtendIDOfPerformanceV2',
            data: { PerformanceExtendID: $("#PerformanceExtendID").val(),PerformanceID:PerformanceID},
            type: 'POST',
            success: function (data) {

            }

        })

    }
    function CreateOrUpdatePerformanceExtend()
    {
        debugger;
        if (PerformanceID=="")
        {
            ShowNotificationProcessor("Lưu Đánh Giá Trước");
            return;
        }
        onCreate($('#form_Eva_PerformanceExtend'), '@_Hrm_Hre_Service', 'api/Eva_PerformanceExtend/');
        $("#PerformanceExtendID").val($("#ID").val());
          UpdatePerformanceExtendIDOfPerformance();
    }

</script>


