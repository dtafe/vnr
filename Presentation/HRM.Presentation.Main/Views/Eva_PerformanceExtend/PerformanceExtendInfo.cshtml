    Guid? PerformanceID = Guid.Empty;
    if (ViewData["PerformanceID"] != null)
    {
        PerformanceID = Guid.Parse(ViewData["PerformanceID"].ToString());
    }
    IList<SelectListItem> lstMobility = Enum.GetValues(typeof(Mobility))
     .Cast<Mobility>()
     .Select(x => new SelectListItem { Value = x.ToString(), Text = EnumDropDown.GetEnumDescription(x) }).OrderBy(p=>p.Text)
     .ToList();
    IList<SelectListItem> lstProficiency = Enum.GetValues(typeof(Proficiency))
    .Cast<Proficiency>()
    .Select(x => new SelectListItem { Value = x.ToString(), Text = EnumDropDown.GetEnumDescription(x) }).OrderBy(p=>p.Text).ToList();
    
    IList<SelectListItem> lstThisYear = Enum.GetValues(typeof(ThisYear))
     .Cast<ThisYear>()
     .Select(x => new SelectListItem { Value = x.ToString(), Text = EnumDropDown.GetEnumDescription(x) }).OrderBy(p=>p.Text)
     .ToList();

    IList<SelectListItem> lstOption = Enum.GetValues(typeof(Option))
     .Cast<Option>()
     .Select(x => new SelectListItem { Value = x.ToString(), Text = EnumDropDown.GetEnumDescription(x) }).OrderBy(p=>p.Text)
     .ToList();
    IList<SelectListItem> lstMeasureType = Enum.GetValues(typeof(MeasureType))
   .Cast<MeasureType>()
   .Select(x => new SelectListItem { Value = x.ToString(), Text = EnumDropDown.GetEnumDescription(x) }).OrderBy(p=>p.Text).ToList();


   //employee career interested
    IList<SelectListItem> lstCareerOption = Enum.GetValues(typeof(Option))
     .Cast<Option>()
     .Select(x => new SelectListItem { Value = x.ToString(), Text = EnumDropDown.GetEnumDescription(x) }).OrderBy(p=>p.Text)
     .ToList();

    IList<SelectListItem> lstRiskToLose = Enum.GetValues(typeof(RiskToLose))
 .Cast<RiskToLose>()
 .Select(x => new SelectListItem { Value = x.ToString(), Text = EnumDropDown.GetEnumDescription(x) }).OrderBy(p=>p.Text)
 .ToList();


    IList<SelectListItem> lstUrgencyToMove = Enum.GetValues(typeof(UrgencyToMove))
 .Cast<UrgencyToMove>()
 .Select(x => new SelectListItem { Value = x.ToString(), Text = EnumDropDown.GetEnumDescription(x) }).OrderBy(p=>p.Text)
 .ToList();
}
