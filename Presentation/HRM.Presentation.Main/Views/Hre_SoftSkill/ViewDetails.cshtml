@using HRM.Presentation.UI.Controls.Kendo
@using HRM.Presentation.UI.Controls.Kendo.DropDownList
@using HRM.Presentation.UI.Controls.Kendo.MultiSelect
@using HRM.Presentation.UI.Controls.Kendo.DropDownList

@model HRM.Presentation.Hr.Models.Hre_SoftSkillModel

@Html.ValidationSummary(false)
@Html.AntiForgeryToken()


<div class="VnRMain">
    <table style="width:50%">
        <tbody>
            <tr>
                <td class="Box_Top_Left"></td>
                <td valign="middle" class="Box_Top_Mid"></td>
                <td class="Box_Top_Right"></td>
            </tr>
            <tr>
                <td class="Box_Mid_Left">&nbsp;&nbsp;&nbsp;</td>
                <td valign="top">
                    <div id="div2">
                        <div class="FieldTitle">
                            @Html.VnrLabelFor(mode => mode.ProfileID)
                        </div>
                        <div class="FieldValue">
                            @Html.DisplayFor(model => model.ProfileName)
                        </div>
                        <div style="clear: both;"></div>
                    </div>
                    <div id="div3">
                        <div class="FieldTitle">
                            @Html.VnrLabelFor(mode => mode.SoftSkillName)
                        </div>
                        <div class="FieldValue">
                            @Html.DisplayFor(model => model.SoftSkillName)
                        </div>
                        <div style="clear: both;"></div>
                    </div>
                    <div id="div4">
                        <div class="FieldTitle">
                            @Html.VnrLabelFor(mode => mode.SoftSkillType)
                        </div>
                        <div class="FieldValue">
                            @Html.DisplayFor(model => model.SoftSkillType)
                        </div>
                        <div style="clear: both;"></div>
                    </div>
                    <div id="div5">
                        <div class="FieldTitle">
                            @Html.VnrLabelFor(mode => mode.SoftSkillLevel)
                        </div>
                        <div class="FieldValue">
                            @Html.DisplayFor(model => model.SoftSkillLevel)
                        </div>
                        <div style="clear: both;"></div>
                    </div>
                    <div id="div6">
                        <div class="FieldTitle">
                            @Html.VnrLabelFor(mode => mode.TrainingPlace)
                        </div>
                        <div class="FieldValue">
                            @Html.DisplayFor(model => model.TrainingPlace)
                        </div>
                        <div style="clear: both;"></div>
                    </div>
                    <div id="div7">
                        <div class="FieldTitle">
                            @Html.VnrLabelFor(mode => mode.TrainingFrom)
                        </div>
                        <div class="FieldValue">
                            @Html.DisplayFor(model => model.TrainingFrom)
                        </div>
                        <div style="clear: both;"></div>
                    </div>
                    <div id="div8">
                        <div class="FieldTitle">
                            @Html.VnrLabelFor(mode => mode.TrainingTo)
                        </div>
                        <div class="FieldValue">
                            @Html.DisplayFor(model => model.TrainingTo)
                        </div>
                        <div style="clear: both;"></div>
                    </div>
                    <div id="div9">
                        <div class="FieldTitle">
                            @Html.VnrLabelFor(mode => mode.Certificate)
                        </div>
                        <div class="FieldValue">
                            @Html.DisplayFor(model => model.Certificate)
                        </div>
                        <div style="clear: both;"></div>
                    </div>
                </td>
                <td class="Box_Mid_Right">&nbsp;&nbsp;&nbsp;&nbsp;</td>
            </tr>
            <tr>
                <td class="Box_Bottom_Left"></td>
                <td class="Box_Bottom_Mid"></td>
                <td class="Box_Bottom_Right"></td>
            </tr>
        </tbody>
    </table>
</div>
