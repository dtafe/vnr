                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value">' + result.Overtime3Hours + '</div> </div>';
                    } else {
                        str += '<div class="div-parent">'
                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable"></div> '
                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value"></div> </div>';
                    }
                    if (result.Overtime4Name != null) {
                        str += '<div class="div-parent">'
                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable">' + result.Overtime4Name + ' </div> '
                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value">' + result.Overtime4Hours + '</div> </div>';
                    } else {
                        str += '<div class="div-parent">'
                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable"></div> '
