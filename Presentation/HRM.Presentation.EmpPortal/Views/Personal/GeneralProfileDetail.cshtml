<script type="text/javascript">
    var TabsCurrent = "Hre";//Lưu tabstrip đang show ra
    var _tmp = 0;//biến tạm
    var countModule = 6;//bến lưu số lượng của button module
    //biến để lưu các tên của gird và kỳ công hiện tại cho Module Att
    var Att_GridName = [
        //["gridTab_TAMScanLogView", ""],
        ["gridTab_WorkDayView", ""],
        ["gridTab_RosterView", ""],
        ["gridTab_LeavedayView", ""],
        ["gridTab_OvertimeView", ""],
        ["gridAtt_AttendanceTableItem", ""],
        ["gridPayroll_BasicSalary", ""],
        ["gridPayroll_Paysips", ""]
    ];
    //biến để lưu các tên của gird và kỳ công hiện tại cho Module Pal
    var Pal_GridName = [
        //["gridPayroll_BasicSalary", ""],
        //["gridPayroll_BasicSalary", ""],
        ["gridPayroll_Paysips", ""]
    ];

    $("#Hre").kendoTabStrip({
        animation: {
            open: { effects: "fadeIn" }
        },
        select: ChangeTabstripModule,
        activate: ActiveTabstrip,
        contentUrls: [
                        '@Url.Action("BasicInfo","Personal")',
                        '@Url.Action("PersonalInfo","Personal")',
                        '@Url.Action("ContactInfo","Personal")',
                        '@Url.Action("RelativeInfo", "Personal")',
        ]
    });

    $("#Contract").kendoTabStrip({
        animation: {
            open: { effects: "fadeIn" }
        },
        select: ChangeTabstripModule,
        activate: ActiveTabstrip,
        contentUrls: [
                         '@Url.Action("Contact", "Personal")',
        ]
    });


    $("#Insurance").kendoTabStrip({
        animation: {
            open: { effects: "fadeIn" }
        },
        select: ChangeTabstripModule,
        activate: ActiveTabstrip,
        contentUrls: [
                        '@Url.Action("InsuranceInfo", "Personal")',
        ]
    });
    $("#WorkHistory").kendoTabStrip({
        animation: {
            open: { effects: "fadeIn" }
        },
        select: ChangeTabstripModule,
        activate: ActiveTabstrip,
        contentUrls: [
                        '@Url.Action("WorkHistory", "Personal")',
                        '@Url.Action("HDTJob", "Personal")',
        ]
    });
    $("#Att").kendoTabStrip({
        animation: {
            open: { effects: "fadeIn" }
        },
        select: ChangeTabstripModule,
        activate: ActiveTabstrip,
        contentUrls: [
                        //'Url.Action("TAMScanLog", "Personal")',
                        '@Url.Action("WorkDay", "Personal")',
                        '@Url.Action("Roster", "Personal")',
                        '@Url.Action("LeaveDay", "Personal")',
                        '@Url.Action("Overtime", "Personal")',
                        '@Url.Action("ProfileAttendanceDetail", "Personal")',
        ]
    });
    $("#Eva").kendoTabStrip({
        animation: {
            open: { effects: "fadeIn" }
        },
        select: ChangeTabstripModule,
        activate: ActiveTabstrip,
        contentUrls: [
                        '@Url.Action("SaleEvaluation", "Personal")',
        ]
    });
    $("#Pal").kendoTabStrip({
        animation: {
            open: { effects: "fadeIn" }
        },
        select: ChangeTabstripModule,
        activate: ActiveTabstrip,
        contentUrls: [
                        //'Url.Action("SalaryInfo", "Personal")',
                        //'Url.Action("BasicSalary", "Personal")',
                        '@Url.Action("Paysips", "Personal")',
        ]
    });

    //Ẩn hiện các tabstrip
    $("li.btnModule").click(function () {
        debugger;
        var e = this.attributes[1].value;
        if (e != TabsCurrent) {
            $("#" + TabsCurrent).addClass("hidden");
            $("li.btnModule").removeClass("btn-primary");
            $("#" + e).removeClass("hidden");
            if (this.attributes[2].value.indexOf("btnModule") == -1) {
                this.attributes[0].value += " btn-primary";
            }
            else {
                this.attributes[2].value += " btn-primary";
            }
            TabsCurrent = e;
        }

        //reload content breadcrumb
        $("#btnModule").html(this.textContent.trim());
        var tabsName = $('#' + e).data("kendoTabStrip").select().text();
        $("#TabsTripInfo").html(tabsName);

    });
    //load lại các tabstrip đang
    function ChangeTabstripModule(e) {
        debugger;
        var indexTabs = e.contentElement.id.split('-')[1] - 1;
        var CutOffID_Att = $('#CutOffDuration_Att').val();
        var CutOffID_Pal = $('#CutOffDuration_Pal').val();
        if (e.contentElement.id.split('-')[0] == "Att")//trường hợp khi select tabs Module Att
        {
            if (CutOffID_Att != Att_GridName[indexTabs][1]) {//nếu kỳ công khác kỳ công đã load thì load lại
                if (Att_GridName[indexTabs][1] != "")//Nếu chưa load lần nào, tức là load lần đầu tiên
                {
                    $('#' + Att_GridName[indexTabs][0]).data('kendoGrid').dataSource.read();
                }
                Att_GridName[indexTabs][1] = CutOffID_Att;
            }
        }
        if (e.contentElement.id.split('-')[0] == "Pal")//trường hợp khi select tabs Module Pal
        {
            if (CutOffID_Pal != Pal_GridName[indexTabs][1]) {//nếu kỳ công khác kỳ công đã load thì load lại
                if (Pal_GridName[indexTabs][1] != "")//Nếu chưa load lần nào, tức là load lần đầu tiên
                {
                    $('#' + Pal_GridName[indexTabs][0]).data('kendoGrid').dataSource.read();
                }
                Pal_GridName[indexTabs][1] = CutOffID_Pal;
            }
        }
    };
    //Get name tabstrip select
    function ActiveTabstrip(e) {
        if (_tmp == countModule) {//loại bỏ trường hợp cập nhật tên trên breadcrumb khi chạy lần đầu tiên
            var tabsName = $('#' + e.contentElement.id.split('-')[0]).data("kendoTabStrip").select().text();//lấy tên của tabs
            $("#TabsTripInfo").html(tabsName);//hiển thị tên lên breadcrumb
            if (e.contentElement.id == "Att-6")//nếu là tab ngoại lệ này thì load thông tin cho tab
                GetDataForAttendanceTabs();
        }
        else {
            _tmp++;
        }
    };
    //even change dropdown cutoffduration for Module Att
    function CutOffChangeEven_Att(e) {
        debugger;
        var indexTabs = $("#Att").data("kendoTabStrip").select().index();//lấy tab đang selected
        $('#' + Att_GridName[indexTabs][0]).data('kendoGrid').dataSource.read();//load lại gird của tabs đang seleced

        //Load dữ liệu cho tabs của att
        GetDataForAttendanceTabs();
    };
    //even change dropdown cutoffduration for Module Pal
    function CutOffChangeEven_Pal(e) {
        debugger;
        var indexTabs = $("#Pal").data("kendoTabStrip").select().index();//lấy tab đang selected
        $('#' + Pal_GridName[indexTabs][0]).data('kendoGrid').dataSource.read();//load lại gird của tabs đang seleced
    };

    //load dữ liệu cho form của tabs Att-6
    function GetDataForAttendanceTabs() {
        $.ajax({
            type: 'POST',
            url: '@_hrm_Hre_Service' + 'Att_GetData/GetAttendanceTableByProIDandCutOID',
