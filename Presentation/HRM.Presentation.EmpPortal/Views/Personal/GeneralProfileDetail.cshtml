                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable">' + result.LeaveDay2Name + ' </div> '
                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value">' + result.LeaveDay1Hours + ' (h)</div> </div>';
                    } else {
                        strLD += '<div class="div-parent">'
                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable"></div> '
                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value"></div> </div>';
                    }
                    if (result.LeaveDay3Name != null) {
                        strLD += '<div class="div-parent">'
                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable">' + result.LeaveDay3Name + ' </div> '
                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value">' + result.LeaveDay1Hours + ' (h)</div> </div>';
                    } else {
                        strLD += '<div class="div-parent">'
                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable" title="Tên nhân viên"></div> '
                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value"></div> </div>';
                    }
                    if (result.LeaveDay4Name != null) {
                        strLD += '<div class="div-parent">'
                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable" >' + result.LeaveDay4Name + ' </div> '
                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value">' + result.LeaveDay1Hours + ' (h)</div> </div>';
                    } else {
                        strLD += '<div class="div-parent">'
                                + '<div class="col-lg-5 col-md-5 col-sm-5 col-xs-5 lable" ></div> '
                                + '<div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 value"></div> </div>';
                    }
                    
                    
                    $("#divLeaveday").empty();
                    $("#divLeaveday").append(strLD);

                }
            },
            error: function (result) {
                debugger;
                window.ShowNotificationProcessor("Lỗi trong quá trình xử lý !");
            }
        });
    };


    function ReloadTabsTrip(id) {
        var tabStrip = $("#" + id).kendoTabStrip().data("kendoTabStrip");
        var index = tabStrip.select();
        tabStrip.reload(index);
    };

</script>

















