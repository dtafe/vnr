@using HRM.Infrastructure.Utilities;
@using HRM.Presentation.UI.Controls.Kendo
@{
    ViewBag.Title = "GeneralProfileDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var _hrm_Hre_Service = ConstantPathWeb.Hrm_Hre_Service;
    var profileId = Guid.Empty;
    if (Model != null && Model.ID != null)
    {
        profileId = Model.ID;
    }
}
@model HRM.Presentation.EmpPortal.Models.Hre_ProfileModelPortal

<script>
    $('.k-widget').css('box-sizing', '').css('-webkit-box-sizing', '');

    //Read data for grid module Att
    function filterField_Att(isExport) {
        debugger;
        var param = {
            CutOffDurationID: $('#CutOffDuration_Att').val(),
            ProfileID: '@profileId',
        };
        return param;
    };

    function filterField_Sal(isExport) {
        debugger;
        var param = {
            CutOffDurationID: $('#CutOffDuration_Pal').val(),
            ProfileID: '@profileId',
        };
        if (isExport == true) {
            param["ExportId"] = $('#ddlScreenName').val();
        }
        return param;
    };
</script>
